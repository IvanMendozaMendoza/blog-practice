---
import "@/styles/global.css";
import { ClientRouter } from "astro:transitions";
interface Props {
    title: string;
}
const { title } = Astro.props;
---

<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width" />
        <meta name="generator" content={Astro.generator} />
        <link
            rel="alternate"
            type="application/rss+xml"
            title="The biecks blog"
            href={new URL("rss.xml", "https://bieck-astro-blog.netlify.app")}
        />
        <ClientRouter/>
        <title>Blog</title>
    </head>
    <body class="bg-gray-900 text-stone-100 p-2">
        <slot />
    </body>
</html>
